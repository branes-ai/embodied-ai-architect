# Embodied AI Architect - Environment Variables Template
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# SSH Remote Backend Configuration
# EMBODIED_AI_SSH_HOST=your-gpu-server.example.com
# EMBODIED_AI_SSH_USER=benchmark
# EMBODIED_AI_SSH_KEY=/path/to/your/private/key

# AWS Configuration (if using AWS backends)
# EMBODIED_AI_AWS_ACCESS_KEY_ID=your_access_key
# EMBODIED_AI_AWS_SECRET_ACCESS_KEY=your_secret_key
# EMBODIED_AI_AWS_REGION=us-west-2

# Azure Configuration (if using Azure backends)
# EMBODIED_AI_AZURE_SUBSCRIPTION_ID=your_subscription_id
# EMBODIED_AI_AZURE_TENANT_ID=your_tenant_id
# EMBODIED_AI_AZURE_CLIENT_ID=your_client_id
# EMBODIED_AI_AZURE_CLIENT_SECRET=your_client_secret

# GCP Configuration (if using GCP backends)
# EMBODIED_AI_GCP_PROJECT_ID=your_project_id
# EMBODIED_AI_GCP_SERVICE_ACCOUNT_KEY=/path/to/service-account.json

# Kubernetes Configuration (if using K8s backends)
# EMBODIED_AI_K8S_KUBECONFIG=/path/to/kubeconfig
# EMBODIED_AI_K8S_NAMESPACE=benchmarks

# Database Configuration (if using database storage)
# EMBODIED_AI_DB_HOST=localhost
# EMBODIED_AI_DB_PORT=5432
# EMBODIED_AI_DB_NAME=embodied_ai
# EMBODIED_AI_DB_USER=embodied_ai
# EMBODIED_AI_DB_PASSWORD=your_password

# Vault Configuration (if using HashiCorp Vault)
# VAULT_ADDR=https://vault.example.com:8200
# VAULT_ROLE_ID=your_role_id
# VAULT_SECRET_ID=your_secret_id

# Development Settings
# EMBODIED_AI_DEBUG=false
# EMBODIED_AI_LOG_LEVEL=INFO
